{
  "name": "ol-nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "storybook build -o ./out",
    "dev": "storybook dev -p 6006",
    "lint": "next lint",
    "lint-staged": "npm run lint && lint-staged",
    "prettier": "prettier --write './**/*.{ts,tsx}'",
    "build-storybook": "storybook build",
    "publish-storybook": "npm run build-storybook && touch ./storybook-static/.nojekyll && gh-pages -d storybook-static -t true",
    "update": "ncu -u && npm install"
  },
  "lint-staged": {
    "./**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@tweakpane/core": "^2.0.5",
    "@tweakpane/plugin-essentials": "^0.2.1",
    "@types/three": "^0.179.0",
    "bowser": "^2.12.1",
    "file-saver": "^2.0.5",
    "gh-pages": "^6.3.0",
    "local-storage": "^2.0.0",
    "n8ao": "^1.10.1",
    "next": "15.5.2",
    "paper": "^0.12.18",
    "postprocessing": "^6.37.7",
    "prettier": "^3.6.2",
    "react": "^19",
    "react-dom": "^19",
    "stats-gl": "^3.6.0",
    "three": "^0.179.1",
    "three-bvh-csg": "^0.0.17",
    "three-mesh-bvh": "^0.9.1",
    "tweakpane": "^4.0.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.6.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-links": "^9.1.3",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/nextjs": "^9.1.3",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-storybook": "^9.1.3",
    "eslint-plugin-unused-imports": "^4.2.0",
    "ifdef-loader": "^2.3.2",
    "postcss": "^8",
    "storybook": "^9.1.3",
    "tailwindcss": "^4.1.12",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5",
    "worker-loader": "^3.0.8"
