    "next": "14.2.3",
    "paper": "^0.12.17",
    "postprocessing": "^6.36.4",
    "react": "^18",
    "react-dom": "^18",
    "stats-gl": "^3.3.0",
    "three": "^0.164.1",
    "tweakpane": "^4.0.4"
